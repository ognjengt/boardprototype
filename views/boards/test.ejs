<h1>Testing tebrice</h1>
<h4 class="subtitle">Testerica</h4>
<div class="container-fluid">
  <div class="top col-md-12">
      <button class="btn button-blue button-small"><b>Random</b></button>
  </div>
  <div class="group-title">
    <h5><b>All boards</b></h5>
  </div>

  <div class="main-content">
    <div class="row board-row" id="allBoards">
<a href="#">
    <div class="col-sm-6 col-md-4 col-lg-3">
      <div class="board board-Project">
        <h4><b>Board</b></h4>
        <div class="type-title"><h5><b>asdasd</b></h5></div>
        <p>asdasd</p>
      </div>
    </div>
  </a>

  <% boards.forEach(function(board) {%>
    <a href="#">
      <div class="col-sm-6 col-md-4 col-lg-3">
        <div class="board board-<%=board.boardType%>">
          <h4><b><%= board.title%></b></h4>
          <div class="type-title"><h5><b><%= board.boardType%></b></h5></div>
          <p><%= board.description%></p>
        </div>
      </div>
    </a>
  <% }); %>

  </div>
</div>
